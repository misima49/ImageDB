        <div class="container">
            <form action="/" method="post">
                <div class="input-form">
                  <label for="ImgName" class="form-label">Image Name</label>
                  <input type="text" class="form-control" id="ImgNamw" name="imgName" placeholder="Image Name">
                </div>
                <div class="input-form">
                  <label for="ImgURL" class="form-label">Image URL</label>
                  <input type="url" class="form-control" id="ImgURL" name="imgURL" placeholder="Image URL">
                </div>
                <div class="input-form">
                  <label for="ImgDetails" class="form-label">Image Details</label>
                  <textarea class="form-control" id="ImgDetails" rows="3" name="imgDetails" placeholder="Details of the image"></textarea>
                </div>

                <button type="submit" class="btn btn-primary">Add</button>
            </form>
        </div>                



                
    
                
        	
        
       	
